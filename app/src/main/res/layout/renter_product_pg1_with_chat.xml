<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    android:background="@color/light_color_background"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"
    tools:context=".Rent.RentalDetails">






    <TextView
        android:id="@+id/search_background"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#2e303030"
        android:translationZ="90dp"

        android:fontFamily="@font/raleway"


        android:textColor="#FFFFFF"
        android:padding="16dp"
        android:textSize="18sp"
        android:gravity="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <ImageView
        android:id="@+id/share_btn"
        android:layout_width="wrap_content"

        android:translationZ="90dp"
        android:animateLayoutChanges="true"

        android:layout_height="wrap_content"
        android:layout_marginTop="18dp"
        android:layout_marginLeft="220dp"
        android:background="@drawable/share"
        android:fontFamily="@font/roboto"
        android:hint="Share"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />

    <TextView
        android:id="@+id/report_btn"
        android:layout_width="100dp"
        android:animateLayoutChanges="true"

        android:layout_height="36dp"
        android:layout_marginTop="20dp"
        android:layout_marginRight="20dp"
        android:clickable="true"
        android:translationZ="90dp"
        android:fontFamily="@font/roboto_bold"
        android:text="Report Product"
        android:textColor="@color/white"
        android:visibility="invisible"
        app:layout_constraintEnd_toStartOf="@id/share_btn"
        app:layout_constraintTop_toTopOf="parent" />




    <Button
        android:id="@+id/delete_btn"
        android:layout_width="71dp"

        android:translationZ="90dp"
        android:layout_height="36dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="10dp"
        android:background="@drawable/login_btn"
        android:fontFamily="@font/roboto"
        android:hint="Remove"
        android:visibility="invisible"
        app:layout_constraintStart_toEndOf="@id/edit_btn"
        app:layout_constraintTop_toTopOf="parent"
        />

    <Button
        android:id="@+id/edit_btn"
        android:layout_width="71dp"
        android:layout_height="36dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="10dp"
        android:translationZ="180dp"
        android:fontFamily="@font/roboto"
        android:background="@drawable/login_btn"
        android:hint="Edit"
        android:visibility="invisible"
        app:layout_constraintStart_toEndOf="@id/insights_btn"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/insights_btn"
        android:layout_width="71dp"
        android:layout_height="36dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="10dp"
        android:translationZ="90dp"
        android:fontFamily="@font/roboto"
        android:background="@drawable/login_btn"
        android:hint="Insights"
        android:visibility="invisible"
        app:layout_constraintStart_toEndOf="@id/back_btn"
        app:layout_constraintTop_toTopOf="parent" />







    <ImageView
        android:id="@+id/back_btn"
        android:layout_width="44dp"
        android:layout_height="48dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="5dp"
        android:translationZ="90dp"
        android:clickable="true"

        android:src="@drawable/back_btn"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"


        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#F7F7F7  "
            tools:context=".Rent.RentProductMain">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraint_rent_product"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/colorPrimary"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">


                <ImageView
                    android:id="@+id/product_pic_input"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:clickable="true"

                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.503"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />


                <TextView
                    android:id="@+id/product_name_input"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"

                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="128dp"

                    android:fontFamily="@font/raleway_bold"

                    android:textColor="@color/common_google_signin_btn_text_dark_default"
                    android:textColorHint="#00F3F3F3"
                    android:textSize="35dp"
                    app:layout_constraintBottom_toBottomOf="@id/product_pic_input"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent" />

                <TextView
                    android:id="@+id/price2"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"

                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="0dp"
                    android:fontFamily="@font/raleway_bold"

                    android:textColor="@color/common_google_signin_btn_text_dark_default"
                    android:textColorHint="#00F3F3F3"
                    android:textSize="35sp"
                    app:layout_constraintBottom_toBottomOf="@id/product_pic_input"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@id/product_name_input" />


            </androidx.constraintlayout.widget.ConstraintLayout>

            <ImageView
                android:id="@+id/black_background_bar"
                android:layout_width="match_parent"
                android:layout_height="4dp"
                android:background="@color/dark_background"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/constraint_rent_product" />

            <ImageView
                android:id="@+id/black_background_bar_bottom"
                android:layout_width="match_parent"
                android:layout_height="4dp"
                android:background="@color/dark_background"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/product_photos" />


            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/renter_rating_layout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:layout_marginTop="10dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/product_description_input">


                <TextView
                    android:id="@+id/user_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="16dp"
                    android:fontFamily="@font/raleway_bold"
                    android:padding="5dp"
                    android:text="Renter Information"
                    android:textColor="@color/accent_background"
                    android:textSize="28sp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/user_info_btn"
                    android:layout_width="wrap_content"
                    android:layout_height="36dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="110dp"
                    android:fontFamily="@font/roboto_medium"
                    android:gravity="center"
                    android:padding="5dp"
                    android:shadowColor="@color/white"
                    android:text="More Products From This User"
                    android:textColor="@color/accent_background"
                    android:textSize="15dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/city_user_txt" />

                <com.mikhaellopez.circularimageview.CircularImageView
                    android:id="@+id/profile_pic_output"
                    android:layout_width="100dp"
                    android:layout_height="103dp"
                    android:layout_marginTop="48dp"
                    android:clickable="true"
                    android:src="@drawable/insert_photo_icon"
                    app:civ_border_width="0dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="@id/user_title" />

                <TextView
                    android:id="@+id/renter_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="13dp"
                    android:fontFamily="@font/raleway"
                    android:text="User Name"
                    android:textColor="@color/common_google_signin_btn_text_dark_focused"
                    android:textSize="20sp"
                    app:layout_constraintStart_toEndOf="@+id/profile_pic_output"
                    app:layout_constraintTop_toTopOf="@+id/profile_pic_output" />

                <ImageView
                    android:id="@+id/rating_pic"
                    android:layout_width="100dp"
                    android:layout_height="20dp"
                    android:src="@drawable/star0"
                    app:layout_constraintStart_toStartOf="@+id/renter_name"
                    app:layout_constraintTop_toBottomOf="@+id/renter_name" />


                <TextView
                    android:id="@+id/rating_amount_txt"
                    android:layout_width="83dp"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/raleway_light"
                    android:text="# Reviews"
                    android:textColor="@color/common_google_signin_btn_text_dark_focused"
                    android:textSize="14sp"
                    app:layout_constraintEnd_toEndOf="@+id/rating_pic"
                    app:layout_constraintStart_toStartOf="@+id/rating_pic"
                    app:layout_constraintTop_toBottomOf="@+id/rating_pic" />

                <TextView
                    android:id="@+id/city_user_txt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="75dp"
                    android:layout_marginTop="10dp"
                    android:clickable="true"
                    android:fontFamily="@font/raleway"
                    android:text="City"
                    android:textColor="@color/common_google_signin_btn_text_dark_focused"

                    android:textSize="18sp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/rating_amount_txt" />

            </androidx.constraintlayout.widget.ConstraintLayout>



            <Button
                android:id="@+id/completeTransactionBtn"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_marginTop="30dp"
                android:fontFamily="@font/raleway_bold"
                android:gravity="center"

                android:background="@drawable/background_right_orange"
                android:text="Complete Transaction"
                android:textAllCaps="false"
                android:textColor="@color/white"
                android:paddingHorizontal="20dp"
                android:textSize="25sp"

                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.501"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/black_background_bar_bottom" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/condition_box"
                android:layout_width="0dp"
                android:layout_height="wrap_content"

                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/completeTransactionBtn"

                android:layout_marginTop="30dp"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp" >

                <TextView
                    android:id="@+id/condition_title"
                    android:layout_width="0dp"
                    app:layout_constraintWidth_percent=".37"
                    android:layout_height="0dp"
                    android:background="@drawable/condition_title_background"
                    android:fontFamily="@font/raleway_bold"

                    android:hint="Condition:"
                    android:padding="11dp"

                    android:textColor="@color/common_google_signin_btn_text_dark_focused"
                    android:textColorHint="@color/common_google_signin_btn_text_dark_focused"
                    android:textSize="15sp"
                    android:textAlignment="center"

                    app:layout_constraintBottom_toBottomOf="@id/condition_txt_view"
                    app:layout_constraintEnd_toStartOf="@id/condition_txt_view"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="@id/condition_txt_view" />

                <TextView
                    android:id="@+id/condition_txt_view"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/condition_spinner_background"
                    android:fontFamily="@font/raleway"
                    android:gravity="center"
                    android:hint="None"
                    android:padding="11dp"
                    android:textColor="@color/common_google_signin_btn_text_dark_focused"
                    android:textColorHint="@color/common_google_signin_btn_text_dark_focused"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@id/condition_title"
                    app:layout_constraintTop_toTopOf="parent" />

            </androidx.constraintlayout.widget.ConstraintLayout>


            <TextView
                android:id="@+id/product_description_title"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:layout_marginTop="12dp"
                android:background="@color/accent_background"
                android:fontFamily="@font/raleway_bold"
                android:inputType="textPersonName"
                android:padding="8dp"
                android:text="Rental Details"
                android:textAlignment="center"
                android:textColor="@color/white"
                android:textSize="23dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/condition_box" />

            <TextView
                android:id="@+id/product_description_input"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:background="@drawable/outline_dark"
                android:fontFamily="@font/raleway"

                android:inputType="textMultiLine"
                android:minLines="1"
                android:nestedScrollingEnabled="true"
                android:padding="16dp"
                android:scrollbarAlwaysDrawVerticalTrack="true"
                android:textAlignment="textStart"
                android:textColor="@color/black_p50"
                android:textColorHint="@color/accent_background"
                android:textSize="17dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/product_description_title" />



            <ViewFlipper
                android:id="@+id/product_photos"
                android:layout_width="match_parent"
                android:layout_height="200dp"
                android:layout_weight="1"

                android:contentDescription="Product Photos"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/black_background_bar" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </ScrollView>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="24dp"
        android:background="#00ffffff"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:id="@+id/barConstraint"
        android:visibility="visible"
        android:translationZ="90dp"

        >

        <ImageView
            android:id="@+id/home_bar"
            android:layout_width="330dp"
            android:layout_height="70dp"

            android:src="@drawable/background_left"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/rent"
            android:layout_width="129dp"
            android:layout_height="wrap_content"

            android:background="@drawable/background_right"

            android:clickable="true"
            android:fontFamily="@font/raleway_bold"
            android:gravity="center"
            android:text="Chat Now"
            android:textAllCaps="false"
            android:textColor="#393939"
            android:textSize="17dp"
            app:layout_constraintBottom_toBottomOf="@+id/home_bar"
            app:layout_constraintEnd_toEndOf="@+id/home_bar"
            app:layout_constraintHorizontal_bias="0.125"
            app:layout_constraintStart_toStartOf="@+id/home_bar"
            app:layout_constraintTop_toTopOf="@+id/home_bar" />

        <Button
            android:id="@+id/cancel"
            android:layout_width="129dp"
            android:layout_height="wrap_content"

            android:background="@drawable/background_right_cancel"

            android:clickable="true"
            android:fontFamily="@font/raleway_bold"
            android:gravity="center"
            android:text="Cancel"
            android:textAllCaps="false"
            android:textColor="#F2F2F2"
            android:textSize="17dp"
            app:layout_constraintBottom_toBottomOf="@+id/home_bar"
            app:layout_constraintEnd_toEndOf="@+id/home_bar"
            app:layout_constraintHorizontal_bias="0.875"
            app:layout_constraintStart_toStartOf="@+id/home_bar"
            app:layout_constraintTop_toTopOf="@+id/home_bar" />

        <ProgressBar
            android:id="@+id/progress_bar"
            android:layout_width="30dp"
            android:layout_height="30dp"
            app:layout_constraintBottom_toBottomOf="@+id/rent"
            app:layout_constraintEnd_toEndOf="@+id/rent"
            app:layout_constraintHorizontal_bias="0.494"
            app:layout_constraintStart_toStartOf="@+id/rent"
            app:layout_constraintTop_toTopOf="@+id/rent" />


    </androidx.constraintlayout.widget.ConstraintLayout>

    <ImageView
        android:id="@+id/darkenBackground"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#99000000"
        android:visibility="gone"/>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/continueConstraint"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:background="#00ffffff"
        android:layout_marginBottom="24dp"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:visibility="gone"
        >

        <ImageView
            android:id="@+id/home_bar2"
            android:layout_width="330dp"
            android:layout_height="0dp"
            app:layout_constraintHeight_percent="1"

            android:src="@drawable/background_left"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="70dp"
            android:visibility="gone"
            android:layout_height="30dp"
            android:src="@drawable/ic_baseline_keyboard_arrow_down_24"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/home_bar2"
            app:layout_constraintStart_toStartOf="@+id/home_bar2" />

        <Button
            android:id="@+id/rent2"
            android:layout_width="129dp"
            android:layout_height="wrap_content"

            android:background="@drawable/background_right"

            android:clickable="true"
            android:fontFamily="@font/raleway_bold"
            android:gravity="center"
            android:text="Rent Now"
            android:textAllCaps="false"
            android:textColor="#393939"
            android:textSize="17dp"

            app:layout_constraintEnd_toEndOf="@+id/home_bar2"
            app:layout_constraintHorizontal_bias="0.069"
            app:layout_constraintStart_toStartOf="@+id/home_bar2"
            app:layout_constraintTop_toBottomOf="@+id/proximity_seekbar"
            android:visibility="gone"
            android:layout_marginTop="12dp"
            />

        <TextView
            android:id="@+id/price32"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:clickable="true"
            android:fontFamily="@font/raleway_medium"
            android:gravity="center"
            android:text="Total Price: $0"

            android:textColor="@color/white"

            android:layout_marginTop="2dp"
            android:visibility="gone"
            android:textSize="17dp"
            app:layout_constraintEnd_toEndOf="@+id/home_bar2"
            app:layout_constraintStart_toEndOf="@+id/rent2"
            app:layout_constraintTop_toTopOf="@+id/rent2" />

        <TextView
            android:id="@+id/distance2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:clickable="true"
            android:fontFamily="@font/raleway"
            android:gravity="center"

            android:layout_marginBottom="2dp"

            android:text="Return MM/DD/YYYY"
            android:textColor="@color/white"
            android:visibility="gone"
            android:textSize="14dp"
            app:layout_constraintBottom_toBottomOf="@+id/rent2"

            app:layout_constraintEnd_toEndOf="@+id/home_bar2"
            app:layout_constraintStart_toEndOf="@+id/rent2" />

        <ProgressBar
            android:id="@+id/progress_bar2"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:visibility="gone"
            app:layout_constraintBottom_toBottomOf="@+id/rent2"
            app:layout_constraintEnd_toEndOf="@+id/rent2"
            app:layout_constraintStart_toStartOf="@+id/rent2"
            app:layout_constraintTop_toTopOf="@+id/rent2" />


        <CalendarView
            android:id="@+id/calendarView"
            android:layout_width="300dp"
            android:layout_height="300dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/calendar_background"
            android:contextClickable="true"
            android:visibility="visible"
            android:maxDate="12/31/2021"
            android:minDate="1/1/2021"

            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/calendartitle" />

        <SeekBar
            android:id="@+id/proximity_seekbar"
            android:theme="@style/SeekbarTheme"
            android:layout_width="240dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:visibility="gone"
            android:thumbTint="#d5d5d5"
            app:layout_constraintStart_toStartOf="@+id/calendarView"
            app:layout_constraintTop_toBottomOf="@+id/calendarView"
            app:layout_constraintEnd_toStartOf="@id/proximity_seekbar_txt_output"/>

        <TextView
            android:id="@+id/proximity_seekbar_txt_output"
            android:layout_width="60dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/raleway_bold"
            android:text="0 Days"
            android:textAlignment="center"
            android:visibility="gone"
            android:textSize="14dp"
            android:textColor="#ffffff"

            app:layout_constraintBottom_toBottomOf="@+id/proximity_seekbar"
            app:layout_constraintEnd_toEndOf="@+id/calendarView"
            app:layout_constraintStart_toEndOf="@+id/proximity_seekbar"
            app:layout_constraintTop_toTopOf="@+id/proximity_seekbar" />

        <TextView
            android:id="@+id/calendartitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:visibility="gone"
            android:clickable="true"
            android:fontFamily="@font/raleway_bold"
            android:gravity="center"
            android:text="Select a Pick Up Date"

            android:textColor="@color/white"


            android:textSize="17dp"
            app:layout_constraintEnd_toEndOf="@+id/home_bar2"
            app:layout_constraintStart_toStartOf="@+id/home_bar2"
            app:layout_constraintTop_toTopOf="@+id/home_bar2" />


